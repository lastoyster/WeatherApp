<!DOCTYPE html>
<html lang="en">

<head>
    {{>headerLinks}}
</head>

<body>
    {{>navbar}}

    <html class="main_header">

    <body class="row">
        <div class="col-md-10 col-12 mx-auto">

            <div class="container-fluid main_content">

                <form class="temp_form">

                    <input type="text" id="cityName" placeholder="Emter your city name.." autocomplete="off">
                    <br>
                    <input type="submit" value="search" id="submitBtn">
                    <p id="temp_err"></p>
                </form>

                <div class="tempInformation ">
                    <div class="top_layer">
                        <p id="day">Tuesday</p>
                        <p id="today_date">12 March</p>
                    </div>
                    <div class="main_layer">
                        <p id="city_name">Get output here</p>
                        <div class="middle_layer data_hide">
                            <p id="temp"><span id="temp_real_val">0</span><sup>o</sup>C</p>
                            <p id="temp_status"><i class="fa fa-cloud" aria-hidden="true"></i></p>
                        </div>

                    </div>
                </div>
            </div>

        </div>

        <footer>
            <p>
                created with love by lastOyster
            </p>
        </footer>
    <script stc="js/main.js"></script>
    </body>
    </html>